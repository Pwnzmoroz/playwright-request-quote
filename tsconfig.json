{
    "compilerOptions": {
      "target": "ES2019",
      "module": "commonjs",
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "build",
      "strict": true,
      "rootDir": ".",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "exclude": [
      "node_modules"
    ]
  }